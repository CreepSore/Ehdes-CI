{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Web-Interface",
            "program": "${workspaceFolder}/Web-Interface/app.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch CI-Agent",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\Build-Agent\\ci-agent.js",
            "args": [
                "build",
                "workspace=Hitman-2-Hack"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Communicator",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\Agent-Communicator\\launcher.js",
            "args": []
        }
    ]
}